<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane styleClass="paneClass"
            maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="780.0"
            prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.SceneControl.MainMenuController">

   <Button styleClass="buttonClass"
           fx:id="startGameBtn"
           layoutX="382.0"
           layoutY="376.0"
           mnemonicParsing="false"
           prefHeight="66.0"
           prefWidth="525.0"
           text="START"
           onMouseClicked="#startGame">
   </Button>

   <Button styleClass="buttonClass"
           fx:id="exitGameBtn"
           layoutX="379.0"
           layoutY="486.0"
           mnemonicParsing="false"
           prefHeight="50.0"
           prefWidth="531.0"
           text="EXIT"
           onMouseClicked="#exit">
   </Button>

   <Label styleClass="labelClass"
          layoutX="452.0"
          layoutY="158.0"
          prefHeight="105.0"
          prefWidth="354.0"
          text="Dash Effect"
          textAlignment="CENTER">
   </Label>

   <stylesheets>
      <URL value="@../css/testing.css"/>
   </stylesheets>
</AnchorPane>
